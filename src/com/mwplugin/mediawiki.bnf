    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-mode-parameter ::=
	image-mode-manual-thumb|
	image-mode-auto-thumb|
	image-mode-frame|
	image-mode-frameless
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-mode-manual-thumb ::=
	"thumbnail=" image-name
	"thumb=" image-name
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-name ::=
	page-name "." image-extension
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-mode-auto-thumb ::=
	"thumbnail"|
	"thumb"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-mode-frame ::=
	"framed"|
	"enframed"|
	"frame"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-mode-frameless ::=
	"frameless"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-size-parameter ::=
	digits "px"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-align-parameter ::=
	image-align-left|
	image-align-center|
	image-align-right|
	image-align-none
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-align-left ::=
	"left"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-align-center ::=
	"center"|
	"centre"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-align-right ::=
	"right"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-align-none ::=
	"none"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

image-valign-parameter ::=
