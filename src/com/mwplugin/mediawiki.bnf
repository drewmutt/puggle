//	r|
//	s|
//	t|
//	u|
//	v|
//	w|
//	x|
//	y|
//	z
symbol ::=
	html-unsafe-symbol|
	underscore|
	"."|
	","|
	"("|")"|"-"|":"|"#"|"/"|"'"|";"|"–"|"\""|"!"|"%"|"?"|"_"|"—"|"+"|"$"|"~"|"·"|"’"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
html-unsafe-symbol ::=
	unescapedampersand|
	unescapedlessthan|
	unescapedgreaterthan
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
html-entity ::=
	"&#" decimal-number ";"|
	"&#x" hex-number ";"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
decimal-number ::=
	decimaldigit+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
hex-number ::=
	hex-digit+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
hex-digit ::=
	decimaldigit|
	"A"|
	"B"|
	"C"|
	"D"|
	"E"|
	"F"|
	"a"|
	"b"|
	"c"|
	"d"|
	"e"|
	"f"

{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

internal-link-start ::=
	"[["

{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
article-link ::=
	(interwiki-prefix | ":")? namespace-prefix? article-title

{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
interwiki-prefix ::=
	interwiki ":"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

interwiki ::=
	"Wikipedia"|"File"

{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
namespace-prefix ::=
	namespace? ":"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

namespace ::=
	letter+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

article-title ::=
	page sub-page*
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
page ::=
	page-first-char page-char*
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

page-first-char ::=
	ucaseletter|
	lcaseletter |
    decimaldigit|
    underscore
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}


page-char ::=
	canonical-page-char|
	space
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

canonical-page-char ::=
	letter|
	decimaldigit|
	symbol
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

sub-page ::=
	sub-page-separator page-char+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

sub-page-separator ::=
	"/"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

title-legal-char ::= lcaseletter
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}


internal-link-end ::=
	"]]"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

article ::=
	(special-block-and-more | paragraph-and-more)+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
special-block-and-more ::=
	special-block (newline special-block-and-more | newline? paragraph-and-more)?
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
special-block ::=
	table|
	horizontal-rule |
	list-item |
	template-block |
    section
//	space-block |
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

template-block ::= "{{" template-name whitespace? newline*  ("|" whitespace? (template-property-named|template-property-unnamed)? whitespace? newline* noparse-block?)* "}}"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

template-property-named ::= template-property-name whitespace? equals whitespace? template-property-value
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

template-property-unnamed ::= template-property-value
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

template-name ::= inline-text
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}
template-property-value ::= ((special-block-and-more whitespace?)+ | url | inline-text)
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

template-property-name ::= inline-text
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

horizontal-rule ::=
	"----" dashes? inline-text? newline
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

dashes ::=
	"-"+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

inline-text ::=
	inline-element*
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}


inline-element ::=
	url|
	category-link |
	link |
	magic-link |
	image-inline |
	gallery-block |
	media-inline |
	text-with-formatting |
	template-block
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

category-link ::=
	internal-link-start category-namespace ":" article-title (pipe sort-key)* internal-link-end
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

category-namespace ::=
	"Category"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

sort-key ::=
	single-line-any-content
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

single-line-any-content ::=
    ( inline-element | special-block | decimaldigit | letter | symbol | space-tab | equals)*
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

unicode-character ::=
	any-supported-unicode-character Whitespaces?
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

any-supported-unicode-character ::=
	letter
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

Whitespaces ::=
	newline|
	TAB|
	space
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

link ::=
	internal-link|
	external-link
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

internal-link ::=
	internal-link-start article-link ("#" section-id)? ((pipe variable-name "=" inline-text) | (pipe whitespace? inline-text))* internal-link-end extra-description?
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

variable-name ::=
    (ucaseletter|lcaseletter) (ucaseletter|decimaldigit|lcaseletter)*

section-id ::=
	(title-legal-char | "%" | "#")*
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

link-description ::=
	inline-text
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

extra-description ::=
	letter+
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

external-link ::=
	openbracket url whitespace? text-with-formatting? closebracket
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

url ::=
	protocol url-path
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

protocol ::=
	"http://"|
	"https://"|
	"ftp://"|
	"ftps://"|
	"www."|
	"mailto:"
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

url-path ::=
	url-char
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

url-char ::=
	LEGAL_URL_ENTITY
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

whitespace ::=
	whitespace-char whitespace?
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}


magic-link ::=
	isbn|
	rfc-number|
	pmid-number
{
    mixin="com.mwplugin.psi.impl.MediaWikiNamedElementImpl"
    implements="com.mwplugin.psi.IMediaWikiNamedElement"
}

isbn ::=
	"ISBN" spaces digits
